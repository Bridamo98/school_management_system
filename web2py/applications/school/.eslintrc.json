{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "parser": "@babel/eslint-parser",
    "rules": {
        "@typescript-eslint/strict-boolean-expressions": "warn",
        "@typescript-eslint/prefer-nullish-coalescing": "warn"
    },
    "requireConfigFile": false
}
